{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
