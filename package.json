{
  "name": "@hyrious/import-cost",
  "version": "0.1.4",
  "description": "Get package size of import statements, powered by esbuild",
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "keywords": [
    "import",
    "gzip",
    "size",
    "bundle",
    "esbuild"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "bin": "cli.mjs",
  "files": [
    "src",
    "dist",
    "cli.mjs"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --treeshake --target esnext --dts"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.0",
    "builtin-modules": "^5.0.0",
    "escalade": "^3.2.0",
    "gzip-size": "^7.0.0",
    "pretty-bytes": "^7.0.0",
    "quick-lru": "^7.0.1",
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^24.0.10",
    "esbuild": "^0.25.6",
    "eslint": "^9.30.1",
    "lodash-es": "^4.17.21",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
